{
  "name": "phisch/oauth-server",
  "type": "library",
  "description": "An OAuth 2.0 authorization and resource server library.",
  "keywords": [
    "oauth",
    "oauth2",
    "oauth2.0"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Schaffrath",
      "email": "github@philipp.schaffrath.email",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~5.6|~7.0",
    "symfony/http-foundation": "^3.1",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "~4.0||~5.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Phisch90\\OAuth\\Server\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phisch90\\OAuth\\Server\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
